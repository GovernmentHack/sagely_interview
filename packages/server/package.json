{
	"name": "@sagely_interview/server",
	"version": "1.0.0",
	"private": true,
	"description": "Template TS Express server with debug environment",
	"main": "src/index.js",
	"scripts": {
		"test": "jest --reporters=default --reporters=jest-junit --env=node /src",
		"start": "node dist/src/index.js",
		"build": "npm run clean && npm run compile",
		"compile": "tsc",
		"clean": "rm -rf dist && rm -rf tsconfig.buildinfo",
		"watch-node": "nodemon --watch dist --inspect=127.0.0.1:1337 dist/src/index.js",
		"watch-ts": "tsc -w --preserveWatchOutput",
		"watch": "concurrently -k -n \"Typsecript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\""
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"cors": "^2.8.5",
		"csv-parse": "^5.3.3",
		"dotenv": "16.0.3",
		"express": "4.18.2",
		"mongodb": "^4.12.1"
	},
	"devDependencies": {
		"@types/cors": "^2.8.12",
		"@types/csv-parse": "^1.2.2",
		"@types/express": "4.17.14",
		"@types/jest": "29.2.3",
		"@types/node": "18.11.9",
		"@types/supertest": "2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"concurrently": "7.6.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^27.1.6",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "29.3.1",
		"jest-environment-node": "29.3.1",
		"jest-junit": "15.0.0",
		"nodemon": "2.0.20",
		"supertest": "6.3.1",
		"ts-jest": "29.0.3",
		"typescript": "4.9.3"
	}
}
